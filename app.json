{
  "expo": {
    "name": "SplitTripBills",
    "slug": "SplitTripBills",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SplitTripBills_Logo.png",
    "scheme": "splittripbills",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/SplitTripBills_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplitTripBills_Logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aidenthien.SplitTripBills"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SplitTripBills_Logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e66e935-1327-4d57-bac6-e21b98d31968"
      }
    }
  }
}